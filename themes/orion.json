	"tokenColors": [
		{
			"scope": ["source"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment"],
			"settings": {
				"foreground": "#5c5c5c"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#e0bc87"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#c25664"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.character",
				"constant.regexp",
				"constant.rgb-value",
				"constant.language",
				"constant.other"
			],
			"settings": {
				"foreground": "#c25664"
			}
		},
		{
			"scope": ["new.expr entity.name"],
			"settings": {
				"foreground": "#a77283"
			}
		},
		{
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#a77283"
			}
		},
		{
			"scope": ["variable"],
			"settings": {
				"foreground": "#a77283",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage", "storage.type"],
			"settings": {
				"foreground": "#e7813d"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.assignment",
				"storage.type.function.arrow",
				"meta.template.expression punctuation",
				"punctuation.separator.key-value",
				"meta.object-literal.key meta.brace.square",
				"meta.template.expression keyword.operator",
				"keyword.operator.or.regexp",
				"keyword.operator.quantifier",
				"punctuation.definition.group.regexp",
				"punctuation.definition.character-class"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Library class/type",
			"scope": ["support.type", "support.class"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a77283"
			}
		},
		{
			"name": "Class name",
			"scope": ["entity.name.class"],
			"settings": {
				"foreground": "#a77283"
			}
		},
		{
			"name": "Inherited class",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#C8AE9D"
			}
		},
		{
			"name": "Function name",
			"scope": ["entity.name.function"],
			"settings": {
				"fontStyle": "",
				"foreground": "#EBEBFF"
			}
		},
		{
			"name": "Function argument",
			"scope": ["variable.parameter"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e0bc87"
			}
		},
		{
			"name": "Tag name",
			"scope": ["entity.name.tag"],
			"settings": {
				"fontStyle": "",
				"foreground": "#c25664"
			}
		},
		{
			"name": "Tag attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"fontStyle": "",
				"foreground": "#d18466"
			}
		},
		{
			"name": "Library Objects",
			"scope": ["support.class", "support.other", "support.variable"],
			"settings": {
				"fontStyle": "",
				"foreground": "#c25664"
			}
		},
		{
			"name": "Library constant",
			"scope": ["support.constant"],
			"settings": {
				"fontStyle": "",
				"foreground": "#d18466"
			}
		},

		{
			"name": "Library variable / functions",
			"scope": ["support.function", "support.other.variable"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Markup Quote",
			"scope": ["markup.quote"],
			"settings": {
				"foreground": "#d18466"
			}
		},
		{
			"name": "Markup Bold",
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#d18466",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italic",
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#d18466",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup Inline",
			"scope": ["markup.inline.raw"],
			"settings": {
				"foreground": "#C8AE9D"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": ["markup.heading.setext"],
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#d18466"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#A79DF8"
			}
		}
	]
}
