{
	"name": "orion",
	"displayName": "Orion Dark",
	"version": "0.1.0",
	"icon": "./assets/logo.png",
	"description": "A beautiful darkish theme for Visual Studio Code.",
	"categories": [
		"Themes"
	],
	"publisher": "schulke-214",
	"repository": {
		"type": "git",
		"url": "https://github.com/schulke-214/vscode-orion-theme.git"
	},
	"author": "Maximilian Schulke <hello@maximilianschulke.com>",
	"license": "MIT",
	"keywords": [
		"Orion",
		"Theme",
		"Dark",
		"Fire",
		"Space"
	],
	"scripts": {
		"generate": "ncc run ./generate.ts -q"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"contributes": {
		"themes": [
			{
				"label": "Orion",
				"uiTheme": "vs-dark",
				"path": "./themes/orion.json"
			}
		]
	},
	"dependencies": {
		"@types/node": "^12.7.12",
		"polished": "^3.4.1",
		"vscode-theme-generator": "^0.1.5"
	},
	"devDependencies": {
		"@zeit/ncc": "^0.20.5"
	}
}
